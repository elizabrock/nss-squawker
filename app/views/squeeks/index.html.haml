.row
  .small-9.columns
    %h2.subheader SQUEEK!

- if user_signed_in?
  = render partial: "squeeks/new"
- else
  .row
    .small-9.columns
      %p Sign in or Sign up to post a squeek
.row
  .small-9.columns
    %ul.squeeks.squawked
      - @squeeks.each do |squeek|
        - if squeek.viewable_by? current_user
          = render squeek
    = paginate @squeeks
